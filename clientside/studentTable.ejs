<head>
    <center>
    <h1>Student</h1>
    </center>
      <title>Student Tables</title>

      <style >
          th{
            margin: 5px;
            padding: 10px;
            font-size: 25px;
            font-weight: bold;
            min-width: 150px;

          }
          table{
            margin: 30px ;
          }
      </style>
   </head>
    
   <body>
    <center>
      <table border = "1">
         <tr>
            <th>Name</th>
            <th>SRN</th>
            <th>Email</th>
            <th>Mob.no</th>
            <th>Branch</th>
            <th>Semester</th>
         </tr>
         <% for(let i=0; i<data.length; i++){ %>
         <tr>
            <td><%= data[i].name %></td>
            <td><%= data[i].SRN %></td>
            <td><%= data[i].email %></td>
            <td><%= data[i].mobile_number %></td>
            <td><%= data[i].Branch %></td>
            <td><%= data[i].semester %></td>
            
         </tr>
         <% } %>
     
      </table>
  </center>
      
   </body>
</html>